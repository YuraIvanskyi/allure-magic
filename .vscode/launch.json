    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "name": "Launch GlobalInfoWidget",
            "request": "launch",
            "mainClass": "i1skyi.GlobalInfoWidget",
            "projectName": "global-info"
        },
        {
            "type": "java",
            "name": "Launch Current File",
            "request": "launch",
            "mainClass": "${file}"
        },
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true
        }
    ]
}